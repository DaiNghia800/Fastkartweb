@using Fastkart.Models.Entities;
@using System.Text.Json

@{
    Layout = "_Layout";
    ViewData["Title"] = "Home Page";
    var newProduct = ViewData["newProduct"] as List<Product>;
    var featureProduct = ViewData["featureProduct"] as List<Product>;
    var totalNewProduct = newProduct != null ? newProduct.Count : 0;
    var totalFeatureProduct = featureProduct != null ? featureProduct.Count : 0;
}

<section class="home-1">
    <div class="container">
        <div class="row">

            <!-- Left banner -->
            <div class="col-md-8">
                <div class="text-md-start wrap-left">
                    <div class="col-md-6">
                        <h6 class="title-letter">ORGANIC</h6>
                        <h1 class="title-main">100% Fresh</h1>
                        <p class="para-pro">Fruit & Vegetables</p>
                        <p class="para-process">Free shipping on all your order. We deliver you enjoy</p>
                        <button class="btn btn-process btn-animation position-relative">Shop Now</button>
                    </div>
                </div>
            </div>

            <!-- Right banner -->
            <div class="col-md-4 ration_medium">
                <div class="home-contain bg-size position-relative h-100">
                    <div class="text-center p-4 bg-success text-white rounded wrap-right">
                        <h3 class="title">Fresh & 100% Organic <br> farmer's market</h3>
                        <button class="btn-white btn-light text-theme mt-3">Shop Now</button>
                    </div>
                </div>
            </div>

        </div>
    </div>
</section>
<section class="home-2">
    <div class="container">
        <div class="wrap-ct-bt d-flex align-items-center justify-content-between">
            <h2 class="title-section">Shop By Categories</h2>
            <div class="wrap-btn">
                <div class="custom slick-prev-custom"></div>
                <div class="custom slick-next-custom"></div>
            </div>
        </div>
        <div class="row">
            <div class="col-12">
                <div class="category-slider">
                    @{
                        if(ViewData["categories"] != null)
                        {
                            var i = 1;
                            foreach(ProductCategory productCategory in ViewBag.categories)
                            {
                                <div class="item">
                                    <div class="shop-category-box">
                                        <a href="/product-category/@productCategory.Slug"
                                           class="d-flex align-items-center justify-content-center circle-@i position-relative">
                                                @{
                                                    var thumbnails = string.IsNullOrWhiteSpace(productCategory.Thumbnail)
                                                    ? new List<string>()
                                                    : JsonSerializer.Deserialize<List<string>>(productCategory.Thumbnail);

                                                    var firstImg = thumbnails.FirstOrDefault();
                                                 }

                                                @if (!string.IsNullOrEmpty(firstImg))
                                                {
                                                // <img src="./assets/images/category-2.png" alt="img">
                                                <img src=@firstImg alt=@productCategory.CategoryName class="img img-fluid">
                                                }
                                                
                                        </a>
                                        <div class="category-name">
                                            <h3 class="name text-center">@productCategory.CategoryName</h3>
                                        </div>
                                    </div>
                                </div>
                                i++;
                            }
                        }
                    }
                </div>
            </div>
        </div>
    </div>
</section>
