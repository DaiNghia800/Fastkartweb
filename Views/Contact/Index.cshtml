
@{
    Layout = "_Layout";
    ViewData["title"] = "Contact";
}
<section class="contact-us">
    <div class="container-fluid">
        <div class="">
            <div class="row g-4">
                <div class="col-lg-6 contact-us-left">
                    <div class="left-contact-us">
                        <div class="row">
                            <div class="col-xl-12">
                                <div class="contact-image">
                                    <img src="./assets/images/contact-us.png" alt="">
                                </div>
                            </div>
                            <div class="col-xl-12">
                                <div class="contact-title">
                                    <h3>Get in touch</h3>
                                </div>
                                <div class="contact-detail">
                                    <div class="row g-4">
                                        <div class="col-xxl-6 col-lg-12 col-sm-6">
                                            <div class="contact-detail-box">
                                                <i class="fa-solid fa-phone"></i>
                                                <div class="contact-content">
                                                    <h4>Phone</h4>
                                                    <p>(+1) 618 190 496</p>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-xxl-6 col-lg-12 col-sm-6">
                                            <div class="contact-detail-box">
                                                <i class="fa-solid fa-envelope"></i>
                                                <div class="contact-content">
                                                    <h4>Email</h4>
                                                    <p>geweto9420@chokxus.com</p>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-xxl-6 col-lg-12 col-sm-6">
                                            <div class="contact-detail-box">
                                                <i class="fa-solid fa-location-dot"></i>
                                                <div class="contact-content">
                                                    <h4>London Office</h4>
                                                    <p>Cruce Casa de Postas 29</p>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-xxl-6 col-lg-12 col-sm-6">
                                            <div class="contact-detail-box">
                                                <i class="fa-solid fa-building"></i>
                                                <div class="contact-content">
                                                    <h4>Bournemouth Office</h4>
                                                    <p>Visitación de la Encina 22</p>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-6 contact-us-right">
                    <div class="contact-us-title">
                        <h2>Contact us</h2>
                    </div>
                    <div class="right-contact-us">
                        <div class="row">
                            <div class="col-xxl-6 col-lg-12 col-sm-6">
                                <div class="custom-form">
                                    <label for="">First Name</label>
                                    <div class="custom-input">
                                        <input type="text" placeholder="Enter Your First Name">
                                        <i class="fa-solid fa-user"></i>
                                    </div>
                                </div>
                            </div>
                            <div class="col-xxl-6 col-lg-12 col-sm-6">
                                <div class="custom-form">
                                    <label for="">Last Name</label>
                                    <div class="custom-input">
                                        <input type="text" placeholder="Enter Your Last Name">
                                        <i class="fa-solid fa-user"></i>
                                    </div>
                                </div>
                            </div>
                            <div class="col-xxl-6 col-lg-12 col-sm-6">
                                <div class="custom-form">
                                    <label for="">Email Address</Address></label>
                                    <div class="custom-input">
                                        <input type="text" placeholder="Enter Your Email Address">
                                        <i class="fa-solid fa-envelope"></i>
                                    </div>
                                </div>
                            </div>
                            <div class="col-xxl-6 col-lg-12 col-sm-6">
                                <div class="custom-form">
                                    <label for="">Phone Number</label>
                                    <div class="custom-input">
                                        <input type="text" placeholder="Enter Your Phone Number">
                                        <i class="fa-solid fa-mobile-screen-button"></i>
                                    </div>
                                </div>
                            </div>
                            <div class="col-12">
                                <div class="custom-form">
                                    <label for="">Message</label>
                                    <div class="custom-text">
                                        <textarea placeholder="Enter Your Message"></textarea>
                                        <i class="fa-solid fa-message"></i>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="send-btn ">
                            <button class="btn-animation">Send messenger</button>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<section class="map-section">
    <div class="container-fluid p-0">
        <div class="map-box">
            <iframe src="https://www.google.com/maps/embed?pb=!1m23!1m12!1m3!1d2994.3803116994895!2d55.29773782339708!3d25.222534631321!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!4m8!3e6!4m5!1s0x3e5f43496ad9c645%3A0xbde66e5084295162!2sDubai%20-%20United%20Arab%20Emirates!3m2!1d25.2048493!2d55.2707828!4m0!5e1!3m2!1sen!2sin!4v1652217109535!5m2!1sen!2sin"
                    style="border:0;" allowfullscreen="" loading="lazy"
                    referrerpolicy="no-referrer-when-downgrade">
            </iframe>
        </div>
    </div>
</section>